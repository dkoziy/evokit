@import './mixins.sss'

:root
    --ek-body-background-TEMPLATE_NAME: var(--background)
    --ek-body-color-TEMPLATE_NAME: var(--color)

@mixin media .body_background_TEMPLATE_NAME
    background: var(--ek-body-background-TEMPLATE_NAME)

@mixin media .body_color_TEMPLATE_NAME
    color: var(--ek-body-color-TEMPLATE_NAME)
